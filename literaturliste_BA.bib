	langid = {english},
	keywords = {{BA}, Atlantification},
	file = {Randelhoff et al. - 2018 - Seasonality of the Physical and Biogeochemical Hyd.pdf:C\:\\Users\\David\\Zotero\\storage\\WF86D8LP\\Randelhoff et al. - 2018 - Seasonality of the Physical and Biogeochemical Hyd.pdf:application/pdf}
